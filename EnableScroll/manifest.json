{
  "manifest_version": 2,
  "name": "Example Overflow Extension",
  "version": "1.0",
  "description": "Sets overflow to auto for all websites.",
  "permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png"
  }
}
